                    </hc:AnimationPath>
                </Viewbox>
                <StackPanel  HorizontalAlignment="Center"
                             Margin="20 0 0 0">
                    <TextBlock Foreground="#023e8a"
                               TextWrapping="Wrap">
                        <Run Text="PulseWatch: Monitoring System"
                                        FontWeight="Bold"
                                        FontSize="13" />
                    </TextBlock>
                    <TextBlock   Foreground="{StaticResource SecondaryTextBrush}"
                                 TextAlignment="Justify"
                                 Opacity="0.7"
                                 TextWrapping="Wrap">
                         <Run Text="    - is a comprehensive platform designed to provide tracking and surveillance capabilities. With advanced monitoring features and an intuitive interface, ensuring continuous vigilance and empowering users with actionable insights."
                                        FontWeight="SemiBold"
                                        FontSize="8" />
                    </TextBlock>
                    <TextBlock FontWeight="SemiBold"
                               TextWrapping="Wrap"
                               Foreground="{StaticResource SecondaryTextBrush}"
                               Text="Freight Process Outsourcing Solutions Inc." />
                </StackPanel>
            </StackPanel>
        </Border>
        <StackPanel VerticalAlignment="Top"
                    Grid.Column="2"
                    Margin="20 0 30 0"
                    Orientation="Vertical"
                    Grid.RowSpan="2">

            <TextBlock HorizontalAlignment="Center"
                       Margin="0 25 0 0"
                       FontSize="20"
                       Text="Welcome back"
                       Foreground="#023e8a"
                       FontWeight="Bold" />

            <TextBlock  Margin="0 0 0 15"
                        Text="Enter your username and password to sign in"
                        FontWeight="SemiBold"
                        FontSize="10"
                        HorizontalAlignment="Center"
                        Foreground="{StaticResource SecondaryTextBrush}" />

            <hc:TextBox hc:InfoElement.Placeholder="Please enter your username"
                        hc:TitleElement.Title="Username"
                        hc:InfoElement.ShowClearButton="True"
                        hc:InfoElement.Necessary="True"
                        Margin="0 0 0 0"
                        Name="TextBoxUsername"
                        Text="{Binding Username, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
                        KeyDown="TextBox_KeyDown">
                <Validation.ErrorTemplate>
                    <ControlTemplate>
                        <StackPanel>
                            <Border BorderBrush="Red"
                                    BorderThickness="0">
                                <AdornedElementPlaceholder />
                            </Border>
                            <TextBlock Text="{Binding [0].ErrorContent}"
                                       Foreground="Red"
                                       Margin="5 0 0 0" />
                        </StackPanel>
                    </ControlTemplate>
                </Validation.ErrorTemplate>
            </hc:TextBox>

            <hc:PasswordBox hc:InfoElement.Title="Password"
                            hc:InfoElement.Placeholder="Please enter your password"
                            hc:InfoElement.Necessary="True"
                            ShowEyeButton="True"
                            Margin="0 10 0 10"
                            Name="TextBoxPassword"
                            KeyDown="TextBoxPassword_KeyDown"
                            Loaded="TextBoxPassword_Loaded" />

            <hc:TextBox hc:InfoElement.Placeholder="Please enter your desire counts"
                        hc:TitleElement.Title="Target Counts"
                        hc:InfoElement.ShowClearButton="True"
                        hc:InfoElement.Necessary="True"
                        x:Name="TargetTB"
                        KeyDown="TargetTB_KeyDown"
                        Text="{Binding Target, UpdateSourceTrigger=PropertyChanged}" />

            <TextBlock Margin="0 15">                    
                <Hyperlink TextDecorations="None"
                           Command="{Binding ResetPasswordCommand}">Reset Password</Hyperlink>
            </TextBlock>
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>

                <StackPanel Grid.Column="1"
                            Orientation="Horizontal"
                            HorizontalAlignment="Right">
                    <Button Content="Cancel"
                            Height="30"
                            BorderThickness="0"
                            BorderBrush="{StaticResource DangerBrush}"
                            Background="{StaticResource DangerBrush}"
                            Foreground="White"
                            Name="ButtonCancel"
                            Margin="5 0"
                            FontWeight="SemiBold"
                            Click="ButtonCancel_Click" />
                    <ToggleButton IsEnabled="{Binding Loading, Mode=TwoWay,FallbackValue=true, TargetNullValue=true, Converter={StaticResource ReverseBooleanConverter}}"
                                  IsChecked="{Binding Loading, Mode=TwoWay,FallbackValue=false, TargetNullValue=false}"
                                  Style="{StaticResource ToggleButtonLoadingPrimary}"
                                  Content="Login"
                                  Height="30"
                                  FontWeight="SemiBold"
                                  BorderThickness="0"
                                  Name="ButtonLogin"
                                  Click="ButtonLogin_Click" />
                </StackPanel>
            </Grid>
        </StackPanel>
    </Grid>
</hc:Window>
